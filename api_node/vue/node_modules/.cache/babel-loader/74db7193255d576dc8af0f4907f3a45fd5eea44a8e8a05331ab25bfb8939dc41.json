{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      posts: []\n    };\n  },\n  methods: {\n    add: function () {\n      const datas = {\n        name: 'te',\n        doc: 'te',\n        about: 'te',\n        active: 'te',\n        site: 'te'\n      };\n      axios.post('http://localhost:4000/api/inserirData', datas).then(json => {\n        console.log(this.addName, this.addDoc, this.addabout, this.addActive, this.addSite);\n        console.log(json.data);\n      }).catch(err => console.log(err));\n    }\n  },\n  mounted() {\n    axios.get('http://localhost:4000/api/buscaEmpresa').then(json => {\n      console.log(this.posts = json.data.user);\n    });\n  }\n\n  // method:{\n  //    salvar(){\n  //       alert(this.insert.name)\n  //    }\n  // }\n};","map":{"version":3,"mappings":"AAwEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAc;EAEZC,IAAI,GAAE;IACJ,OAAM;MACFC,KAAK,EAAC;IAEV;EACF,CAAC;EAEDC,OAAO,EAAC;IACNC,GAAG,EAAC,YAAU;MAEZ,MAAMC,KAAI,GAAI;QACZC,IAAI,EAAC,IAAI;QACTC,GAAG,EAAC,IAAI;QACRC,KAAK,EAAC,IAAI;QACVC,MAAM,EAAC,IAAI;QACXC,IAAI,EAAC;MACP;MAGEV,KAAI,CACHW,IAAI,CAAC,uCAAuC,EAACN,KAAK,EAClDO,IAAI,CAACC,IAAI,IAAE;QACRC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,OAAO,EAAE,IAAI,CAACC,MAAM,EAAC,IAAI,CAACC,QAAQ,EAAC,IAAI,CAACC,SAAS,EAAC,IAAI,CAACC,OAAM,CAAG;QACjFN,OAAO,CAACC,GAAG,CAACF,IAAI,CAACZ,IAAI,CAAC;MAC1B,CAAC,EACAoB,KAAK,CAACC,GAAE,IAAKR,OAAO,CAACC,GAAG,CAACO,GAAG,CAAC,CAAC;IACnC;EACF,CAAC;EAEDC,OAAO,GAAE;IACPvB,KAAI,CACHwB,GAAG,CAAC,wCAAwC,EAC5CZ,IAAI,CAACC,IAAI,IAAE;MACVC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACb,KAAI,GAAIW,IAAI,CAACZ,IAAI,CAACwB,IAAI,CAAC;IAC1C,CAAC,CAAC;EACJ;;EAIA;EACA;EACA;EACA;EACA;AAEF","names":["axios","data","posts","methods","add","datas","name","doc","about","active","site","post","then","json","console","log","addName","addDoc","addabout","addActive","addSite","catch","err","mounted","get","user"],"sourceRoot":"","sources":["C:\\vue\\hello-world\\src\\App.vue"],"sourcesContent":["\n<template>\n  <div id=\"app\">\n\n