 const datas = {\n        name:'te',\n        doc:'te',\n        about:'te',\n        active:'te',\n        site:'te'\n      }\n        \n    \n        axios\n        .post('http://localhost:4000/api/inserirData',datas)\n        .then(json=>{\n            console.log(this.addName, this.addDoc,this.addabout,this.addActive,this.addSite  )\n            console.log(json.data);\n        })\n        .catch(err => console.log(err));\n    }\n  },\n\n  mounted(){\n    axios\n    .get('http://localhost:4000/api/buscaEmpresa')\n    .then(json=>{\n      console.log(this.posts = json.data.user);\n    });\n  }\n\n\n\n  // method:{\n  //    salvar(){\n  //       alert(this.insert.name)\n  //    }\n  // }\n\n}\n</script>\n\n\n\n\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}